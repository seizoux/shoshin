{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "module": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "./static/js",
    "allowJs": true,
    "checkJs": false,
    "noEmit": false
  },
  "include": [
    "./static/ts/**/*"
  ]
}